@using Smartstore.Admin.Models.Logging
@using Smartstore.Core.Logging
 
@model LogListModel

<div class="form-group">
    <smart-label asp-for="LogLevelId" />
    <select asp-for="LogLevelId" asp-items="Html.GetLocalizedEnumSelectList(typeof(LogLevel)).Skip(1)" asp-placeholder="@T("Admin.Common.All")"></select>
</div>
<div class="form-group">
    <smart-label asp-for="CreatedOnFrom" />
    <editor asp-for="CreatedOnFrom" />
</div>
<div class="form-group">
    <smart-label asp-for="CreatedOnTo" />
    <editor asp-for="CreatedOnTo" />
</div>
<div class="form-group">
    <smart-label asp-for="Logger" />
    <editor asp-for="Logger" asp-template="SearchExpression" />
</div>
<div class="form-group">
    <smart-label asp-for="Message" />
    <editor asp-for="Message" asp-template="SearchExpression" />
</div>