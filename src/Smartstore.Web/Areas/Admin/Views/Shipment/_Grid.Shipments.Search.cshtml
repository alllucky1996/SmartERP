@model ShipmentListModel

<div class="form-group">
    <smart-label asp-for="StartDate" />
    <editor asp-for="StartDate" />
</div>
<div class="form-group">
    <smart-label asp-for="EndDate" />
    <editor asp-for="EndDate" />
</div>
<div class="form-group">
    <smart-label asp-for="TrackingNumber" />
    <editor asp-for="TrackingNumber" asp-template="SearchExpression" />
</div>
<div class="form-group">
    <smart-label asp-for="ShippingMethod" />
    <input asp-for="ShippingMethod" />
</div>